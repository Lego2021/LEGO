 if (!$AUTH_NAMED_PIPE_SO) { skip No auth_named_pipe plugin;
 }  INSTALL SONAME 'auth_named_pipe';
  eval CREATE USER '$USERNAME' IDENTIFIED WITH named_pipe;
 # Connect using named pipe, correct username connect(pipe_con,localhost,$USERNAME,,,,,PIPE);
 SELECT USER(),CURRENT_USER();
 disconnect pipe_con;
 connection default;
 eval DROP USER '$USERNAME';
  # test invalid user name CREATE USER nosuchuser IDENTIFIED WITH named_pipe;
 connect(pipe_con,localhost,nosuchuser,,,,,PIPE);
 DROP USER nosuchuser;
 UNINSTALL SONAME 'auth_named_pipe';
