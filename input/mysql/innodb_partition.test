 set sql_mode='';
 create table t ( a int not null, b int generated always as (1) virtual, c int generated always as (1) virtual, key (c) ) engine=innodb partition by key (a) partitions 2;
 insert into t(a) values(1);
 select b from t group by c;
  drop table t;
  # Make column b a BLOB create table t ( a int not null, b blob generated always as ("a") virtual, c int generated always as (1) virtual, key (c) ) engine=innodb partition by key (a) partitions 2;
 insert into t(a) values(1);
 select b from t group by c;
  drop table t;
  